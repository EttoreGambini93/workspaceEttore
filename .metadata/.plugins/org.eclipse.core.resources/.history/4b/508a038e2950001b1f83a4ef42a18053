package repeticao;

import javax.swing.JOptionPane;

public class DesafioDo {

	public static void main(String[] args) {
		
	int num1 = 0;
	int num2 = 0;
	
	do {
		num1 = Integer.parseInt(JOptionPane.showInputDialog("Número à ser adivinhado: "));
		num2 = Integer.parseInt(JOptionPane.showInputDialog("Adivinhe o número: "));
	}while(num2!=num1);
		
		
		
		
		
		
		
		
		

	}

}
